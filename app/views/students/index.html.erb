<nav class="navbar">
  <div class="nav-container">
    <div class='student'>
    <h1><%=t('.students')%></h1>
    </div>
  </div>
</nav>
<table class='table-container'>
  <tr class='table-th'>
    <th><%=t('.name')%></th>
    <th><%=t('.lastname')%></th>
    <th><%=t('.age')%></th>
    <th><%=t('.address')%></th>
    <th><%=t('.level')%></th>
    <th><%=t('.study')%></th>
  </tr>
  <% @students.each do |student| %>
  <tr class='user'>
    <td class='name'><%= student.name %></td>
    <td class='name'><%= student.lastname %></td>
    <td class='name'><%= student.calculate_age %></td>
    <td class='name'><%= student.address %></td>
    <td class='name'><%= student.school_level %></td>
    <td class='name'><%= student.goes_to_school %></td>
  </tr>
  <%end%>
</table>
